#parse("default/header.vm")
#set($csrfToken=$request.getAttribute('csrfToken'))
<body class="ui-layout-container">
    <form:form id="_csrf-form" cssStyle="display: none">
		<input type="hidden" name="csrfToken" value="$csrfToken" />
	</form:form>

	<div class="ui-layout-north" style="height:60px;background:#B3DFDA;padding:10px">
		<h1>mark's technic world</h1>
	</div>
	<div class="ui-layout-west" style="width:150px;padding:0px;">
		<div id="nav"   fit="true"  border="false">
			
		</div>
	</div>
	
	<div class="ui-layout-south" style="height:50px;background:#A9FACD;padding:10px;">
		<h1>all right reserved!</h1>
	</div>
	<div class="ui-layout-center" id="tabs" >
	</div>
	<div id="mm"  style="width: 200px;">
		<div id="mm-tabupdate">
			刷新
		</div>
		<div class="menu-sep"></div>
		<div id="mm-tabclose">
			关闭
		</div>
		<div id="mm-tabcloseall">
			全部关闭
		</div>
		<div id="mm-tabcloseother">
			除此之外全部关闭
		</div>
		<div class="menu-sep"></div>
		<div id="mm-tabcloseright">
			当前页右侧全部关闭
		</div>
		<div id="mm-tabcloseleft">
			当前页左侧全部关闭
		</div>
	</div>
</body>
<script type="text/javascript">
	$(function(){
		$('body').layout({ applyDefaultStyles: true });  
		
		$("#mm" ).menu();
	});
</script>
</script>
</html>